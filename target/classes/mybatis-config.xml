<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- Alias 설정 -->
    <typeAliases>
        <!-- 기존 alias -->
        <typeAlias type="com.omiyami.shop.product.ProductVO" alias="product"/>
        <typeAlias alias="user" type="com.omiyami.shop.user.UserVO" />
        <typeAlias alias="order" type="com.omiyami.shop.order.OrderVO" />
        
        <!-- admin 관련 alias -->
        <typeAlias alias="dashboardSummary" type="com.omiyami.shop.admin.vo.DashboardSummaryVO" />
        <typeAlias alias="orderSummary" type="com.omiyami.shop.admin.vo.OrderSummaryVO" />
        <typeAlias alias="adminOrder" type="com.omiyami.shop.admin.vo.AdminOrderVO" />
        <typeAlias alias="inquiry" type="com.omiyami.shop.admin.vo.InquiryVO" />
        <typeAlias alias="review" type="com.omiyami.shop.admin.vo.ReviewVO" />
    </typeAliases>
</configuration>